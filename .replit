#include <iostream>
using namespace std;

int main(int argc, char *argv[]) {
    int base, altura,area;
    cout<<"Ingrese la base "<<endl; //comentario
    cin>>base;
    cout<<"Ingrese la altura"<<endl;
    cin>>altura;
    area=(base*altura)/2;
    cout<<"El area del triangulo es :"<<area<<endl;
    return 0;
}